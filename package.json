  "description": "REST for PostGres",
  "main": "api.js",
  "scripts": {
    "start": "node standalone.js",
    "test": "mocha test/lib/ --require test/bootstrap.js --require blanket  -R html-cov > coverage.html",
    "coverage": "./node_modules/istanbul-harmony/lib/cli.js cover node_modules/mocha/bin/_mocha -- test/lib/* --require test/bootstrap.js"
  },
  "config": {
    "blanket": {
