    "koa": "^1.1.2",
    "koa-cors": "0.0.16",
    "koa-logger": "^1.3.0",
    "koa-parse-json": "^1.0.1",
    "koa-router": "^5.3.0",
    "lodash": "^4.0.0",
    "pg": "^4.4.3",
    "pg-promise": "^2.9.5",
    "squel": "^4.2.2"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "istanbul-harmony": "^0.3.16",
    "mocha": "^2.4.5"
  }
